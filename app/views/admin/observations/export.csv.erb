<% @species.each do |species| %>,<%= species.name %><% end %>
<% @events.each do |event| %><%= event.name %><% @species.each do |species| %><% if @grid.has_key?(species.id) %><% if @grid[species.id].has_key?(event.id) %>,<%= number_with_precision(@grid[species.id][event.id], :precision => 1) %><% else %>,<% end %><% end %><% end %>
<% end %>